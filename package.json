  },
  "author": "Dan Van Brunt <me@danvanbrunt.com>",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.197.0",
    "cfn-lambda": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.18.2",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "prettier": "^1.11.1",
    "prettier-eslint": "^8.8.1",
    "serverless-webpack": "^5.0.0-rc.2",
    "webpack": "^4.1.0",
    "webpack-node-externals": "^1.6.0"
  },
  "babel": {
    "presets": [
      "env",
      "stage-0"
    ],
    "env": {
      "targets": {
        "node": "6.10"
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "globals": {
      "__DEV__": true
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-underscore-dangle": 2,
      "arrow-parens": "off",
      "generator-star-spacing": "off",
      "no-console": "off",
      "radix": "off",
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "trailingComma": "es5"
        }
      ]
    }
  }
